{
  "manifest_version": 3,
  "name": "Oasis",
  "version": "0.0.0.1",
  "description": "A simple Chrome extension using React and TypeScript with Google Auth",
  "key": "",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "permissions": ["identity","identity.email", "tabs"],
  "background": {
    "service_worker": "static/js/background.js"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "oauth2": {
    "client_id": "109198751201-37sv3ij36v3qmc1q6tpd0kf3ksht9g15.apps.googleusercontent.com",
    "scopes": ["profile", "email","https://googleapis.com/auth/contacts"]
  }
}